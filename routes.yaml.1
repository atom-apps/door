---
whitelist:
  - /v1/users/users/profile
  - /v1/auth/pages
  - /v1/auth/logout

authorization:
  - title: 仪表台
    name: dashboard
    path: dashboard
    meta:
      hideInMenu: false
      icon: icon-dashboard
    children:
      - title: 工作空间
        name: Workspace
        path: workspace
        meta:
          hideInMenu: false
  - title: 用户管理
    name: users
    path: users
    meta:
      icon: icon-user
      hideInMenu: false
    children:
      - title: 用户列表
        name: UserList
        path: users
        meta:
          hideInMenu: false
        api:
          - get#/v1/users/users
        children:
          - title: 创建
            name: UserCreate
            path: create
            api:
              - post#/v1/users/users
          - title: 查看
            name: UserView
            path: view/:id
            api:
              - get#/v1/users/users/:id
          - title: 编辑
            name: UserEdit
            path: edit/:id
            api:
              - put#/v1/users/users/:id
              - get#/v1/users/users/:id
          - title: 删除
            name: UserDelete
            path: delete/:id
            api:
